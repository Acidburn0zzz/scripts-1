#!/bin/bash
{
find . -name "*.cmake" -type f -print0 | xargs -0 /bin/rm -f
find . -name "*.a" -type f -print0 | xargs -0 /bin/rm -rf -f
find . -name "*Makefile" -type f -print0 | xargs -0 /bin/rm -f
find . -name "CMakeCache.txt" -type f -print0 | xargs -0 /bin/rm -f
find . -name "CMakeFiles" -type f -print0 | xargs -0 /bin/rm -f
}&>/dev/null

exit 0
